{
  "name": "spiral/keeper",
  "type": "library",
  "description": "Keeper - Admin panel and UI toolkit",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "spiral/framework": "^3.0",
    "spiral/attributes": "^2.8 || ^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.5",
    "spiral/toolkit": "^2.0",
    "spiral/helpers": "^1.0",
    "spiral/code-style": "^1.1",
    "spiral/nyholm-bridge": "^1.2",
    "vimeo/psalm": "^4.27"
  },
  "autoload": {
    "psr-4": {
      "Spiral\\Keeper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spiral\\Tests\\Keeper\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "spiral/composer-publish-plugin": true
    }
  }
}
